<h1>Logs</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Media</th>
      <th>Media type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logs.each do |log| %>
      <tr>
        <td><%= log.date %></td>
        <td><%= log.media_id %></td>
        <td><%= log.media_type %></td>
        <td><%= link_to 'Show', log %></td>
        <td><%= link_to 'Destroy', log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
